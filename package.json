{
  "name": "itemdb",
  "version": "1.0.0",
  "description": "Neopets Open Source Item Database",
  "keywords": [],
  "license": "MPL-2.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npx prisma generate && next build",
    "build-turbo": "npx prisma generate && next build --turbopack",
    "build-proxy": "npx prisma generate --data-proxy && next build",
    "build-intl": "npx --yes github:lucca180/itemdb-intl-compiler ./pages --output ./translation/tree-shake -t ./",
    "ts": "tsc --noEmit --incremental",
    "typecheck": "tsc",
    "start": "PORT=4000 next start",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "postbuild": "next-sitemap"
  },
